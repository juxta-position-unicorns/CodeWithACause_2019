  <DataSets>
    <DataSet Name="Courses">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Semester">
            <Value>=Parameters!Semester.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
   c.PeriodName
  ,c.StartTime
  ,c.EndTime
  ,c.Subject
  ,c.CourseName
  ,i.Name as Instructor
FROM Course c
LEFT JOIN Instructor i
  ON i.UserId = c.InstructorId
WHERE c.Year = @Year AND c.Semester = @Semester
ORDER BY
  c.StartTime, c.PeriodName, c.Subject, c.CourseName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PeriodName">
          <DataField>PeriodName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
          <rd:TypeName>System.TimeSpan</rd:TypeName>
        </Field>
        <Field Name="Subject">
          <DataField>Subject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EndTime">
          <DataField>EndTime</DataField>
          <rd:TypeName>System.TimeSpan</rd:TypeName>
        </Field>
        <Field Name="CourseName">
          <DataField>CourseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Instructor">
          <DataField>Instructor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentYear">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT YEAR(GETDATE()) AS YEAR</CommandText>
      </Query>
      <Fields>
