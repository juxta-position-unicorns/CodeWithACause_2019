  <DataSets>
    <DataSet Name="Courses">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StudentId">
            <Value>=Parameters!StudentId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  CONCAT(Course.[Year], ' ', Course.Semester) AS [Semester]
  ,Course.Subject
  ,Course.CourseName
  ,SubjectLevel.CreditHours
  ,CourseStudent.AwardedGrade
FROM
  Course
  INNER JOIN CourseStudent
    ON CourseStudent.CourseId = Course.CourseId
  LEFT JOIN SubjectLevel
    ON SubjectLevel.CourseName = Course.CourseName
WHERE 
  CourseStudent.StudentId = @StudentId
ORDER BY
  [Semester], Course.Subject, Course.CourseName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Semester">
          <DataField>Semester</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subject">
          <DataField>Subject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CourseName">
          <DataField>CourseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CreditHours">
          <DataField>CreditHours</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AwardedGrade">
          <DataField>AwardedGrade</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Student">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StudentId">
            <Value>=Parameters!StudentId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  Student.UserId
  ,Student.FirstName
  ,Student.LastName
  ,Student.StudentNumber
  ,Student.SchoolName
  ,(SELECT AVG(g.AwardedGrade) FROM CourseStudent g WHERE g.StudentId = @StudentId) AS GPA
FROM
  Student
WHERE Student.UserId = @StudentId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
